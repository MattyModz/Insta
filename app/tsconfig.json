{
  "compilerOptions": {
    "jsx": "react",
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": [
    "src/app/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.js",
    "src/serviceWorker.ts"
  ],
  "exclude": ["node_modules"]
}
